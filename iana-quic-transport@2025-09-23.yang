        reference
          "RFC 9000";
      }
      enum preferred_address {
        description
          "0x0d (permanent)";
        reference
          "RFC 9000";
      }
      enum active_connection_id_limit {
        description
          "0x0e (permanent)";
        reference
          "RFC 9000";
      }
      enum initial_source_connection_id {
