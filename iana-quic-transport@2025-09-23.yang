      }
      enum stateless_reset_token {
        description
          "0x02 (permanent)";
        reference
          "RFC 9000";
      }
      enum max_udp_payload_size {
        description
          "0x03 (permanent)";
        reference
          "RFC 9000";
      }
      enum initial_max_data {
        description
          "0x04 (permanent)";
        reference
          "RFC 9000";
      }
      enum initial_max_stream_data_bidi_local {
        description
          "0x05 (permanent)";
        reference
          "RFC 9000";
      }
      enum initial_max_stream_data_bidi_remote {
        description
          "0x06 (permanent)";
        reference
          "RFC 9000";
      }
      enum initial_max_stream_data_uni {
        description
          "0x07 (permanent)";
        reference
          "RFC 9000";
